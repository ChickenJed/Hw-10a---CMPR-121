#include <iostream>
#include <algorithm>
using namespace std;

int main()
{
	cout << "The greater value of 1 and 2 = " << max(1, 2);
	cout << "\nThe greater value of 2 and 1 = " << max(2, 1);
	cout << "\nThe greater value of 'a' and 'z' = " << max('a','z');
	cout << "\nThe greater value of 3.14 and 2.72 = " << max(3.14, 2.72) << "\n\n";

	system("pause");
	
	return 0;
}